<table id="example2" class="table table-bordered table-hover">
    <thead>
      <tr>
        <th>No.</th>
        <th>Thumbnail</th>
        <th>Nama Game</th>
        <th>Kategory</th>
        <th>Nominal</th>
        <th>status</th>
        <th>Aksi</th>
      </tr>
    </thead>
    <tbody>
      <% for( let i = 0; i < voucher.length; i++ ) { %>
        <tr>
          <td class="text-center"><%= i + 1 %></td>
          <td class="text-center"><img src="/uploads/<%= voucher[i].thumbnail %>" alt="<%= voucher[i].nameGame %>" width="50px"></td>
          <td><%= voucher[i].nameGame %></td>
          <td><%= voucher[i]?.category?.nameCategory ?? '-' %></td>
          <td>
            <% voucher[i].nominals.forEach(nominal => { %>
             <span class="badge badge-info">
                <%= nominal.coinName %> [<%= nominal.coinQuantity %>]
             </span>
            <% }) %>
          </td>
          
          <td class="text-center">
            <% if (voucher[i].status === 'Y') { %>
                <span class="badge badge-success">
                    Aktif
                </span>
            <% } else { %>
                <span class="badge badge-danger">
                    Tidak Aktif
                </span>
            <% } %>
          </td>
          <td>
            <form action="" style="display: inline-block;">
                <button class="btn btn-primary btn-sm">
                    <i class="fas fa-folder"></i>
                    <% if (voucher[i].status === 'Y') { %>
                        Matikan
                    <% } else { %>
                        Hidupkan
                    <% } %>
                </button>
            </form>
            <a href="/voucher/edit/<%= voucher[i]._id %>" class="btn btn-info btn-sm">
              <i class="fas fa-pencil-alt"></i>
              Ubah
            </a>
            <form action="/voucher/delete/<%= voucher[i]._id %>?_method=DELETE" method="POST" style="display: inline-block;">
              <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Hapus
              </button>
            </form>
          </td>
        </tr>
      <% } %>
      
  
  
  
  
  
  
  
  
    </tbody>
  </table>